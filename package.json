{
  "name": "plans",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-main": "tsc --sourceMap -p main.tsconfig.json",
    "build-renderer": "webpack",

    "watch-main": "tsc --watch --sourceMap -p main.tsconfig.json",
    "watch-renderer": "webpack --watch",

    "build": "npm run build-main && npm run build-renderer",
    "watch": "npm run watch-main || npm run watch-renderer",
    "electron": "electron ./dist/main.js",
    "start": "npm run build && npm run electron || npm run watch",
    "lint": "eslint -c .eslintrc --ext .ts ./src"
  },
  "repository": "https://github.com/elumixor/plans",
  "keywords": [],
  "author": "elumixor",
  "license": "",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "electron": "^9.4.0",
    "eslint": "^7.3.1",
    "ts-loader": "^8.0.18",
    "typescript": "^3.9.5",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@pixi/core": "^6.0.1",
    "@types/pixi.js": "5.0.0",
    "pixi.js": "5.3.9"
  }
